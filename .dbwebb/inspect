#
# Inspect, tools to inspect a kmom.
#
# Usage: dbwebb inspect kmom01
#
# Sequence executed:
# oopython(), oopythonkmom01(), oopythonlast()
#



#
# Test oopython general, always executed first
#
oopython()
{
    inspectIntro
}



#
# Test general, always executed last in sequence
#
oopythonlast()
{
    inspectMe "me/flask" "app.cgi" "" "" "flask"
}



#
# Test oopython kmom01
#
oopythonkmom01()
{
    inspectLab "uppgift/python-med-objekt-och-klasser" "lab1" "answer.py" "python3 answer.py"
}



#
# Test oopython kmom02
#
oopythonkmom02()
{
    inspectLab "uppgift/python-med-klass-relationer" "lab2" "answer.py" "python3 answer.py"

}



#
# Test oopython kmom03
#
oopythonkmom03()
{

    inspectExerciseHeader "Kortspelet War" "uppgift/bokningssystem" "$KMOM/war"
    inspectCommand "test.py" "$EXEC_DIR/$KMOM/war" "python3 test.py -v"
    inspectCommand "main.py" "$EXEC_DIR/$KMOM/war" "python3 main.py"

    fileIsReadable "me/kmom03/war/uml.png"
    printUrl "uml.png" "me/kmom03/war"

    inspectExerciseHeader "Skapa sequence diagram" "uppgift/skapa-sequence-diagram2" "$KMOM/uml"
    fileIsReadable "me/kmom03/uml/sequenceDiagram.png"
    printUrl "sequenceDiagram.png" "me/kmom03/uml"

}



#
# Test oopython kmom04
#
oopythonkmom04()
{

    inspectExerciseHeader "Queue" "uppgift/skapa-queue" "$KMOM/queue"
    inspectCommand "main.py" "$EXEC_DIR/$KMOM/queue" "python3 main.py"
    viewFileContent "queue.py" "me/$KMOM/queue"

    fileIsReadable "me/kmom04/queue/sekvens.png"
    printUrl "sekvens.png" "me/kmom04/queue"
    fileIsReadable "me/kmom04/queue/klass.png"
    printUrl "klass.png" "me/kmom04/queue"
    
    
    inspectExerciseHeader "Unordered List" "uppgift/skapa-lista" "$KMOM/list"
    inspectCommand "main.py" "$EXEC_DIR/$KMOM/list" "python3 main.py"
    inspectCommand "test.py" "$EXEC_DIR/$KMOM/list" "python3 test.py"
    viewFileContent "unorderedlist.py" "me/$KMOM/list"
    viewFileContent "exceptions.py" "me/$KMOM/list"

}


#
# Test oopython kmom05
#
oopythonkmom05()
{
    inspectLab "uppgift/python-med-rekursiva-funktioner" "lab4" "answer.py" "python3 answer.py"

    inspectExerciseHeader "unorderedlist" "uppgift/terminalprogram-med-sortering-av-lista" "$KMOM/unorderedlist"
    inspectCommand "test_list.py" "$EXEC_DIR/$KMOM/unorderedlist" "python3 test_list.py"
    inspectCommand "main.py" "$EXEC_DIR/$KMOM/unorderedlist" "python3 main.py"
    viewFileContent "unorderedlist.py" "me/$KMOM/unorderedlist"
}



